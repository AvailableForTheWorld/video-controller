{
    "name": "video-controller",
    "description": "full rich controller extension for video",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "hello.html",
        "default_icon": "hello-world.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                    "default": "Ctrl+Shift+F",
                    "mac": "MacCtrl+Shift+F"
                },
            "description": "Opens hello.html"
        }
    }
}